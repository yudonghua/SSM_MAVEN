<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
  PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
  "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite >
    <rule>
        <note>
            主页
        </note>
        <from>^/$</from>
        <to type="forward">/index</to>
    </rule>
	<rule>
		<note>
        	登录
        </note> 
        <from>^/login(.html)?$</from> 
        <to type="forward">/login</to>
	</rule>
</urlrewrite> 